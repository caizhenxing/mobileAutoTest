<!DOCTYPE html>
<html>
<meta charset="utf-8">
<head th:include="include :: header"></head>
<body class="gray-bg">
	<div class="wrapper wrapper-content animated fadeInRight">
		<div class="row">
			<div class="col-sm-12">
				<div class="ibox-content">
					<form class="form-horizontal m-t" id="svnUserResetPwdForm">
						<input id="userId" name="userId" th:value="${user.userId}"
							type="hidden">
<!-- 						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库ID：</label>
							<div class="col-sm-8">
								<input id="svnRepoId" name="svnRepoId"
									class="form-control" type="text" style="cursor: pointer;"
									onclick="openSvnRepo()" readonly="readonly" placeholder="点击选择SVN仓库">
							</div>
						</div> -->
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库名称：</label>
							<div class="col-sm-8">
								<input id="svnRepoName" name="svnRepoName"
									class="form-control" type="text" style="cursor: pointer;"
									onclick="openSvnRepo()" readonly="readonly" placeholder="点击选择SVN仓库">
							</div>
						</div>
<!-- 						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库路径：</label>
							<div class="col-sm-8">
								<input id="svnRepoPath" name="svnRepoPath"
									class="form-control" type="text" style="cursor: pointer;"
									onclick="openSvnRepo()" readonly="readonly" placeholder="点击选择SVN仓库">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN仓库URL：</label>
							<div class="col-sm-8">
								<input id="svnRepoUrl" name="svnRepoUrl"
									class="form-control" type="text" style="cursor: pointer;"
									onclick="openSvnRepo()" readonly="readonly" placeholder="点击选择SVN仓库">
							</div>
						</div> -->
						<div class="form-group">
							<label class="col-sm-3 control-label">用户姓名：</label>
							<div class="col-sm-8">
								<input id="name" name="name" class="form-control" type="text"
									th:value="${user.name}" readonly="readonly">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">SVN用户名：</label>
							<div class="col-sm-8">
								<input id="svnUserName" name="svnUserName" class="form-control"
									type="text" th:value="${user.username}" readonly="readonly">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">新SVN密码：</label>
							<div class="col-sm-8">
								<input id="svnPassword" name="svnPassword" class="form-control"
									type="password">
							</div>
						</div>
						<div class="form-group">
							<label class="col-sm-3 control-label">请再次输入新SVN密码：</label>
							<div class="col-sm-8">
								<input id="confirm_password" name="confirm_password" class="form-control"
									type="password">
							</div>
						</div>
						<div class="form-group">
							<div class="col-sm-8 col-sm-offset-3">
								<button type="submit" class="btn btn-primary">提交</button>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</div>
	<div th:include="include::footer"></div>
	<script type="text/javascript" src="/js/appjs/svn/svnUser/resetPwd.js">
	</script>
</body>
</html>
